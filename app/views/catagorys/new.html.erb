<h1>New Article</h1>

<%= form_with model: @user do |form| %>
  <div>
    <%= form.label :catagory_name %><br>
    <%= form.text_field :catagory_name %>
    <% @user.errors.full_messages_for(:catagory_name).each do |message| %>
      <div><%= message %></div>
    <% end %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
